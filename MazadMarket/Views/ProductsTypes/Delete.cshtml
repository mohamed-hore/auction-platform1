<h2>@Html.Raw(layout.delete)</h2>

<h3>@Html.Raw(layout.deleteMessage)</h3>
<div>
    <h4>@Html.Raw(layout.typeName)</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.typeName)
        </dt>

        <dd>
